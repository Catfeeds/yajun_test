<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>	
<#include "../../../include/editorCommons.html"> 
<body style="overflow-y: hidden" scroll="no">
<@p.editForm url="${baseUrl}/formPermission/add.do" class="form-horizontal"   permissionModule="FORM_PERMISSION">
		<@p.formRow>
			<@p.formColmd class="col-md-4" title="PERMISSION_MODULE"   checktip="PERMISSION_MODULE_NULL"  permissionCode="PERMISSION_MODULE">
				 <@p.input type="select" excludeSelectOption="true" field="module" options=moduleOptions></@p.input>
			</@p.formColmd>
			<@p.formColmd class="col-md-4" title="PERMISSION_CODE"  checktip="PERMISSION_CODE_CHECK_TIP"   permissionCode="PERMISSION_CODE">
				 <input name="code" class="col-xs-10 col-sm-5 form-control" datatype="s2-50" nullmsg='<@s.m code="PERMISSION_CODE_CHECK_TIP" />'>
			</@p.formColmd>
			<@p.formColmd class="col-md-4" title="PERMISSION_NAME"   checktip="PERMISSION_NAME_CHECK_TIP"  permissionCode="PERMISSION_NAME">
				 <input name="name" class="col-xs-10 col-sm-5 form-control" datatype="s2-50">
			</@p.formColmd>
			<@p.formColmd class="col-md-4" title="PERMISSION_DESCRIPTION"  checktip="PERMISSION_DESCRIPTION_NULL"  permissionCode="PERMISSION_DESCRIPTION">
				<textarea name="description" ignore="ignore"  class="col-xs-10 col-sm-5 form-control" ></textarea>
			</@p.formColmd>
		</@p.formRow>
</@p.editForm>
</body>
</html>