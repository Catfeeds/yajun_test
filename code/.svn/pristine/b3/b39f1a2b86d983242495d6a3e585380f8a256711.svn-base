<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=2.0,user-scalable=no,width=device-width" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<meta name="mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="format-detection" content="telephone=no" />
	<link rel="stylesheet" href="../../mui/css/mui.min.css" />
	<script src="../../mui/js/mui.min.js"></script>
	<script src="../../mui/js/app.js"></script>
	<script src="../../js/jquery-1.8.0.min.js"></script>
	<meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=2.0,user-scalable=no,width=device-width" />
<body>
	<header class="mui-bar mui-bar-nav">
		<h1 class="mui-title">首页</h1>
	</header>
	<div style="z-index: 800;width: 50%;height: 20px;position: absolute;bottom: 0;left:0;" onclick="homePage()"></div>
	<div style="z-index: 800;width: 50%;height: 50px;position: absolute;bottom: 0;right:0;" onclick="setting()"></div>
	<nav class="mui-bar mui-bar-tab">
		<a class="mui-tab-item mui-active">
			<span class="mui-icon mui-icon-home"></span>
			<span class="mui-tab-label">首页</span>
		</a>
		<a class="mui-tab-item" id="navGear">
			<span class="mui-icon mui-icon-gear"></span>
			<span class="mui-tab-label">设置</span>
		</a>
	</nav>
	<div class="page-content">
		<div class="mui-content-padded" style="margin-top: 4em;" id="content_menu"></div>
	</div>
<script>
$(function(){
	var menus =JSON.parse(app.localStorage('menus'));
	var html = "";
	if(menus){
		for(var i in menus){
			html+="<a href="+"'"+menus[i].url+"'"+">";
			html+="<button type='button' style='margin-top: 1em;' class='mui-btn mui-btn-primary mui-btn-block'>"+menus[i].name+"</button>";
			html+="</a>";
		}
	}
	$("#content_menu").append(html);
})
 function homePage(){
   window.location.href="../home/home.html";
 }
 function setting(){
 	window.location.href="../personal-center/personal-center.html";
 }
</script>

</body>
</html>