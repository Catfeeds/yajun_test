<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<#include "../../include/editorCommons.html"> 
</head>	
<body style="overflow-y: hidden" scroll="no">
<@p.editForm url="${baseUrl}/tbLabelManage/add.do" class="form-horizontal"   permissionModule="LABEL">
		<@p.formRow>
			<@p.formColmd title="LABEL_LINE" permissionCode="LABEL_LINE">
				<@p.input type="select" field="tmLineId" options=lineOptions datatype="*" ></@p.input>
			</@p.formColmd>
			<@p.formColmd title="LABEL_EPC_ID" permissionCode="LABEL_EPC_ID">
				<input name="epcId" class="inputxt" datatype="*" maxlength='50' />
			</@p.formColmd>
		</@p.formRow>
		<@p.formRow>
			<@p.formColmd title="LABEL_SALVER_NO" permissionCode="LABEL_SALVER_NO">
				<input name="salverNo" class="inputxt"  datatype="*" maxlength='100' />
			</@p.formColmd>
			<@p.formColmd title="LABEL_BRAND" permissionCode="LABEL_BRAND">
				<input name="brand" class="inputxt"  maxlength='100' />
			</@p.formColmd>
		</@p.formRow>
		<@p.formRow>
			<@p.formColmd title="LABEL_BATCH" permissionCode="LABEL_BATCH">
				<input name="batch" class="inputxt"   maxlength='100' />
			</@p.formColmd>
			<@p.formColmd title="REMARKS" permissionCode="LABEL_BATCH">
				<input name="remarks" class="inputxt"   maxlength='500' />
			</@p.formColmd>
		</@p.formRow>
	</@p.editForm>
</body>
</html>
