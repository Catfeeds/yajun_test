package com.wis.basis.common.utils.jmail;

import java.io.File;

public class Mail {

	private String[] to = null;
	private String from = null;
	private String host = null;
	private File[] attachement = null;
	private String subject = null;
	private String bodyContext = null;

	public String[] getTo() {
		return to;
	}

	public void setTo(String[] to) {
		this.to = to;
	}

	public String getFrom() {
		return from;
	}

	public void setFrom(String from) {
		this.from = from;
	}

	public String getHost() {
		return host;
	}

	public void setHost(String host) {
		this.host = host;
	}

	public File[] getAttachement() {
		return attachement;
	}

	public void setAttachement(File[] attachement) {
		this.attachement = attachement;
	}

	public String getSubject() {
		return subject;
	}

	public void setSubject(String subject) {
		this.subject = subject;
	}

	public String getBodyContext() {
		return bodyContext;
	}

	public void setBodyContext(String bodyContext) {
		this.bodyContext = bodyContext;
	}
}
