package com.wis.basis.report.highcharts;

import java.util.Map;
import java.util.TreeMap;

public class Options3d {

	protected Map<String, Object> content = new TreeMap<String, Object>();

	protected Options3d(Integer alpha) {
		content.put("enabled", true);
		content.put("alpha", alpha);
	}

	/**
	 * 设置外旋转角度
	 * @param beta
	 */
	public void setBeta(Integer beta) {
		content.put("beta", beta);
	}
	
}
