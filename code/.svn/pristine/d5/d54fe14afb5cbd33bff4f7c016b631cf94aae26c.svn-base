     <select id="getInventoryNumber" resultType="java.lang.Integer">
       <![CDATA[SELECT T2.INVENTORY_NUMBER
		  FROM TB_MP_SOURCE_DATA T
		  left join TM_SHEET_METAL_MATERIAL T2
		    on T.MATERIAL_ID = T2.ID
		 where T.status < 2
		   and T.MARK_FOR_DELETE = 0
		   and T.MODEL = #{model}
		   and rownum=1
		 ORDER BY T.ID DESC]]>
    </select>
</mapper>