<script type="text/javascript">
	$(document).ready(function() {
		loadNotificationCountInfo();
		<!-- remove it if no bpm module is enable-->
// 		loadTaskCount();
// 		setInterval(loadTaskCount,1000 * 60);
	});
	function renderMsgtype(value) {
		return entryRenderer('MSG_TYPE', value);
	}
	function logout(url, title) {
		BootstrapDialog.confirm({
	          title: '<@s.m code="INDEX_EXIT"/>',
	          message: title,
	         //„ÄÅtype: BootstrapDialog.TYPE_WARNING, // <-- Default value is BootstrapDialog.TYPE_PRIMARY
	          closable: true, // <-- Default value is false
	          draggable: true, // <-- Default value is false
	          btnCancelLabel: '<@s.m code="BTN_CANCEL"/>', // <-- Default value is 'Cancel',
	          btnOKLabel: '<@s.m code="BTN_CONFIRM"/>', // <-- Default value is 'OK',
	        //  btnOKClass: 'btn-warning', // <-- If you didn't specify it, dialog type will be used,
	          callback: function(result) {
					if (result) {
						window.location = url;
					}
	          }
		});
	}
</script>
</head>

