<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<#include "../../include/editorCommons.html"> 
</head>	
<body style="overflow-y: hidden" scroll="no">
	<@p.editForm url="${baseUrl}/badParts/update.do" class="form-horizontal" idValue=bean.id permissionModule="BAD_PARTS">
		<@p.formRow>
			<@p.formColmd title="BAD_PARTS_ILNE" permissionCode="BAD_PARTS_ILNE">
				<@p.input type="select" disabled="disabled" value="${bean.tmLineId!}" datatype="*" options=lines ></@p.input>
				<input type="hidden" name="tmLineId" value="${bean.tmLineId!}" >
			</@p.formColmd>
			<@p.formColmd title="BAD_PARTS_NO" permissionCode="BAD_PARTS_NO">
				<input name="no" value="${bean.no}" readonly="readonly" class="inputxt" />
			</@p.formColmd>
		</@p.formRow>
		<@p.formRow>
			<@p.formColmd title="BAD_PARTS_NAME" permissionCode="BAD_PARTS_NAME">
				<input name="name" value="${bean.name}" class="inputxt" datatype="*" maxlength='150' />
			</@p.formColmd>
			<@p.formColmd title="BAD_PARTS_REMARKS" permissionCode="BAD_PARTS_REMARKS">
				<input name="remarks" class="inputxt" value="${bean.remarks!}" maxlength='150' />
			</@p.formColmd>
		</@p.formRow>
	</@p.editForm>
</body>
</html>

