<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<#include "../../../include/editorCommons.html"> 
</head>	
<body style="overflow-y: hidden" scroll="no">
<@p.editForm url="${baseUrl}/dataPermission/add.do" class="form-horizontal"   permissionModule="DATA_PERMISSION">
		<@p.formRow>
		<@p.formColmd class="col-md-4" title="PERMISSION_MODULE"  permissionCode="PERMISSION_MODULE">
			 <@p.input type="select" excludeSelectOption="true" field="module" options=moduleOptions></@p.input>
		</@p.formColmd>
		<@p.formColmd class="col-md-4" title="PERMISSION_CODE"  permissionCode="PERMISSION_CODE">
				 <input name="code" class="inputxt"  datatype="s2-50"/>
			</@p.formColmd>
		<@p.formColmd class="col-md-4" title="PERMISSION_RELATION_CODE"  permissionCode="PERMISSION_RELATION_CODE">
				 <input name="relationCode" class="inputxt"  datatype="s2-50"/>
			</@p.formColmd>
		<@p.formColmd class="col-md-4" title="PERMISSION_URL"  permissionCode="PERMISSION_URL">
				 <input name="url" class="inputxt"  datatype="*2-50"/>
		</@p.formColmd>
		<@p.formColmd class="col-md-4" title="PERMISSION_DESCRIPTION"  permissionCode="PERMISSION_DESCRIPTION">
			<textarea name="description" class="inputxt" ignore="ignore" style="width:100%"></textarea>
			</@p.formColmd>
		</@p.formRow>
	</@p.editForm>
</body>
</html>
